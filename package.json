{
  "name": "fwidgets",
  "version": "0.0.1",
  "description": "Create a simple Figma plugin UI with zero UI code.",
  "keywords": [
    "figma",
    "plugin",
    "ui"
  ],
  "author": "John Dunning",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fwextensions/fwidgets.git"
  },
  "bugs": {
    "url": "https://github.com/fwextensions/fwidgets/issues"
  },
  "homepage": "https://github.com/fwextensions/fwidgets#readme",
  "dependencies": {
    "@create-figma-plugin/ui": "^2.6.1",
    "@create-figma-plugin/utilities": "^2.6.1",
    "classnames": "^2.3.2",
    "figma-await-call": "0.0.2",
    "preact": "^10"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^2.6.1",
    "@create-figma-plugin/tsconfig": "^2.6.1",
    "@figma/plugin-typings": "1.76.0",
    "typescript": "^5"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "dev": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "fwidgets",
    "name": "Fwidgets",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}
